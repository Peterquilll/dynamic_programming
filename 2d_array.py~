def main():
    # choice you'll be making
    # also you'll need a "seed" choice which is usually row

    num_rows = 4
    num_cols = 7

    t = {}
    for j in range(num_rows):
        t.append([0 for j in range(num_cols)])

        for i in range =(len(t)):
            for j in range(len(t[0])):
                current = t[i][j]
                print current,

            print('')

if __name__ == "__main__":
    main()
